CLIENT() <=
    "/www.stuff.com/" (ch) .
    "[ch, vid.html]" (page1) .
    "[ch, lolcats.html]" (page2)
;



SERVER(sock) <=
    (new file1, file2, file3)
	! (case T : ' "[sock, lolcats.html]" <file1>
	     [] T : ' "[sock, vid.html]" <file2>
	     [] T : ' "[sock, rick.html]" <file3> )
;


LOAD_BALANCE() <=
    ! (new sock)
	' "/www.stuff.com/" <sock> .
	    (case  T : SERVER<sock>
		[] T : SERVER<sock> )
;

